; LSC2 4 2051 RunTeeSwing

parameter swing: Swing

local variable local1

run-script SendBatViewMessage [7613, g_net_smth_master]
stop-script
while (r_smth_maybe_current_phase == 1 && local1 < 100) {
    ++local1
    stop-script
}
if (r_smth_maybe_current_phase == 2) {
    run-script SendBatViewMessage [MSG-SWING, swing]
} else {
    r_smth_maybe_current_phase = 0
}
free-script
