; LSC2 4 2224

local variable local0
local variable local1

for local0 = 1 to 32 ++ {
    sprite-set-range local0 local0
    sprite-clear
}
if (rBallSprite) {
    run-script ReleaseSprite [rBallSprite]
    rBallSprite = 0
}
local1 = sprite-group-get-x08 25
if (local1) {
    for local0 = 1 to local1[0] ++ {
        run-script ReleaseSprite [local1[local0]]
        local1[local0] = 0
    }
    free-array local1
    sprite-group-select 25
    sprite-group-clear
}
free-script
