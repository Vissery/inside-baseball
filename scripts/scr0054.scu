; SCRP 54

local variable local0
local variable local1
local variable local2
local variable local3

dim array local3 i32[0...0][0...local0] swap=2
if (local1 > 0 || local2 > 0) {
    array-fill-values local3 0 0 1 local0 local1 local2
    local3[0] = local0
} else {
    local3[0] = 0
}
return local3
free-script
