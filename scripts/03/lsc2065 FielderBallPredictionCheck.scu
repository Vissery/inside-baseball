; LSC2 3 2065 FielderBallPredictionCheck

parameter kid: Kid

local variable result

if (gNoErrors) {
    return 1
    free-running-script 0
}
if (gBallVelPhysY) {
    result = gKidData[kid][KD-EYE] * 240000 / gBallVelPhysY
} else {
    result = gKidData[kid][KD-EYE] * 240000
}
if (result > 100) {
    result = 100
}
return (101 - result)
free-script
